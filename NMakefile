	.\src\utf16_to_utf8_one.obj   \
	.\src\utf8_cstd.obj

utf16.a: $(OBJS)
	lib /out:utf16.a $(OBJS)

test.exe: tests\test.c utf16.a
	cl /O2 /I. /DCHECK_UTF8_LOCALE /DLIBC_UCRT /Wall /wd4820 /wd5045 /wd4711 /wd4710 tests\test.c /Fetest.exe /link utf16.a

check: test.exe
	.\test.exe

clean:
	del /q src\*.obj utf16.a test.exe test.obj test.ilk test.pdb
